{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@k-msg/webhook": ["../../packages/webhook/src/index.ts"],
      "@k-msg/webhook/*": ["../../packages/webhook/src/*"]
    },
    "types": ["@cloudflare/workers-types/latest"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src/**/*.ts"]
}
