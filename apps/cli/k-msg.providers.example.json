{
  "defaultProviderId": "sms-rr",
  "providers": [
    {
      "id": "iwinv",
      "module": "@k-msg/provider",
      "exportName": "IWINVProvider",
      "enabledWhenEnv": "IWINV_API_KEY",
      "config": {
        "baseUrl": "https://alimtalk.bizservice.iwinv.kr",
        "smsBaseUrl": "https://sms.bizservice.iwinv.kr",
        "sendEndpoint": "/api/v2/send/"
      },
      "configFromEnv": {
        "apiKey": "IWINV_API_KEY",
        "smsApiKey": "IWINV_SMS_API_KEY",
        "smsAuthKey": "IWINV_SMS_AUTH_KEY",
        "senderNumber": "IWINV_SENDER_NUMBER",
        "ipRetryCount": "IWINV_IP_RETRY_COUNT",
        "ipRetryDelayMs": "IWINV_IP_RETRY_DELAY_MS",
        "ipAlertWebhookUrl": "IWINV_IP_ALERT_WEBHOOK_URL"
      }
    },
    {
      "id": "aligo",
      "module": "@k-msg/provider",
      "exportName": "AligoProvider",
      "enabledWhenEnv": "ALIGO_API_KEY",
      "config": {
        "smsBaseUrl": "https://apis.aligo.in",
        "alimtalkBaseUrl": "https://kakaoapi.aligo.in"
      },
      "configFromEnv": {
        "apiKey": "ALIGO_API_KEY",
        "userId": "ALIGO_USER_ID",
        "senderKey": "ALIGO_SENDER_KEY",
        "sender": "ALIGO_SENDER",
        "friendtalkEndpoint": "ALIGO_FRIENDTALK_ENDPOINT"
      }
    },
    {
      "kind": "router",
      "id": "sms-rr",
      "strategy": "round_robin",
      "providers": ["iwinv", "aligo"],
      "default": true
    }
  ]
}
