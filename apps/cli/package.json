{
  "name": "@k-msg/cli",
  "version": "0.3.0",
  "private": false,
  "description": "k-msg CLI (prebuilt binaries via GitHub Releases)",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "k-msg": "./bin/k-msg.js",
    "kmsg": "./bin/k-msg.js"
  },
  "files": [
    "bin",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "bunli build",
    "build:all": "bunli build --targets all",
    "build:native": "bunli build --targets native",
    "build:js": "bun build src/k-msg.ts --outdir=dist --format=esm --target=bun",
    "dev": "bun --watch src/k-msg.ts",
    "test": "bun test",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@bunli/core": "^0.5.4",
    "@bunli/plugin-ai-detect": "^0.5.2",
    "@bunli/test": "^0.3.2",
    "@types/bun": "catalog:",
    "@types/node": "catalog:",
    "bunli": "^0.5.3",
    "k-msg": "workspace:*",
    "typescript": "catalog:",
    "zod": "catalog:"
  },
  "keywords": [
    "alimtalk",
    "cli",
    "messaging",
    "kakao"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/k-otp/k-msg"
  },
  "license": "MIT"
}
