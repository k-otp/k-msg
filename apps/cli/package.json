{
  "name": "@k-msg/cli",
  "version": "0.1.1",
  "private": true,
  "description": "k-msg CLI tool",
  "type": "module",
  "bin": {
    "k-msg": "./dist/k-msg.js",
    "kmsg": "./dist/k-msg.js"
  },
  "scripts": {
    "build": "bunli build",
    "build:all": "bunli build --targets all",
    "build:native": "bunli build --targets native",
    "build:js": "bun build src/k-msg.ts --outdir=dist --format=esm --target=bun",
    "dev": "bun --watch src/k-msg.ts",
    "test": "bun test",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@bunli/core": "^0.5.4",
    "k-msg": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "bunli": "^0.5.3",
    "typescript": "catalog:",
    "@types/bun": "catalog:",
    "@types/node": "catalog:"
  },
  "keywords": [
    "alimtalk",
    "cli",
    "messaging",
    "kakao"
  ],
  "license": "MIT"
}
